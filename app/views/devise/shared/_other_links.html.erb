
<%- if controller_name != 'sessions' %>
    <!--%= link_to "Log in", new_session_path(resource_name), 'data-toggle' => "modal", 'data-target' => "#loginPopup" , :class=>"button"  %><br /-->
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
    <!--%= link_to "Forgot your password?", new_password_path(resource_name) %><br /-->
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
    <!--%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br /-->
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
    <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br />
<% end -%>
<br />

<%- if devise_mapping.omniauthable? %>
    <%- Devise.omniauth_providers.each do |provider| %>
        <!--%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %><br /-->
        <%- if OmniAuth::Utils.camelize(provider) == "Facebook" %>
            <% @path = "/assets/fb_sign.PNG" %>
        <% end-%>
        <%- if OmniAuth::Utils.camelize(provider) == "GoogleOauth2" %>
            <% @path = "/assets/go_sign.PNG" %>
        <% end-%>
        <%- if OmniAuth::Utils.camelize(provider) == "Twitter" %>
            <% @path = "/assets/tw_sign.PNG" %>
        <% end-%>
        <%- if OmniAuth::Utils.camelize(provider) == "LinkedIn" %>
            <% @path = "/assets/ld_sign.PNG" %>
        <% end-%>
        <%= link_to(image_tag(@path,size: '200x35') , omniauth_authorize_path(resource_name, provider)) %><br /><br />

    <% end -%>
<% end -%>

